<org.eventb.theory.core.operatorArgument name="(" org.eventb.core.expression="BNum" org.eventb.core.identifier="Num2"/>
<org.eventb.theory.core.directOperatorDefinition name=")" org.eventb.theory.core.formula="COND(Num1 BNumLeq Num2, TRUE, TRUE)"/>
