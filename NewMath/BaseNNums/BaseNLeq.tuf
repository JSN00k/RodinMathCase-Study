<org.eventb.theory.core.newOperatorDefinition name="1" org.eventb.core.label="BNumLeq_B" org.eventb.theory.core.associative="false" org.eventb.theory.core.commutative="false" org.eventb.theory.core.formulaType="true" org.eventb.theory.core.notationType="INFIX">
